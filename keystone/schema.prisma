// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id                Int       @id @default(autoincrement())
  name              String    @default("")
  email             String    @unique @default("")
  password          String
  createdAt         DateTime? @default(now())
  from_Account_user Account[] @relation("Account_user")
  from_Email_user   Email[]   @relation("Email_user")
}

model Provider {
  id                    Int       @id @default(autoincrement())
  name                  String    @default("")
  createdAt             DateTime? @default(now())
  from_Account_provider Account[] @relation("Account_provider")
  from_Email_provider   Email[]   @relation("Email_provider")
}

model Account {
  id            Int        @id @default(autoincrement())
  user          User[]     @relation("Account_user")
  provider      Provider[] @relation("Account_provider")
  token         String     @default("")
  refresh_token String     @default("")
  expiry        DateTime?
  createdAt     DateTime?  @default(now())
}

model Email {
  id         Int       @id @default(autoincrement())
  user       User?     @relation("Email_user", fields: [userId], references: [id])
  userId     Int?      @map("user")
  provider   Provider? @relation("Email_provider", fields: [providerId], references: [id])
  providerId Int?      @map("provider")
  subject    String    @default("")
  sender     String    @default("")
  message    String    @default("")
  timestamp  DateTime?
  createdAt  DateTime? @default(now())

  @@index([userId])
  @@index([providerId])
}
